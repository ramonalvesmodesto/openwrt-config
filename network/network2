config interface 'loopback'
        option device 'lo'
        option proto 'static'
        list ipaddr '127.0.0.1/8'

config globals 'globals'
        option packet_steering '0'

config device
        option name 'br-lan'
        option type 'bridge'
        list ports 'lan1'
        list ports 'lan2'
        list ports 'lan3'
        list ports 'lan4'
        list ports 'sfp2'

config interface 'lan'
        option device 'br-lan'
        option proto 'static'
        list ipaddr '192.168.1.1/29'
        option delegate '0'
        option ip6hint '60'
        list dns '127.0.0.53'

config device
        option name 'br-wan'
        option type 'bridge'
        list ports 'sfp1'
        list ports 'wan'

config device
        option name 'sfp1'
        option macaddr '1C:EF:03:B5:70:CA'

config device
        option name 'wan'
        option macaddr '1C:EF:03:B5:70:CA'

config interface 'wan'
        option device 'sfp1'
        option proto 'pppoe'
        option username 'ramonamodesto96'
        option password '120426'
        option ipv6 '0'
        option keepalive '2 5'
        option mtu '1420'
        option peerdns '0'
        option force_link '1'
        option delegate '0'
        option ac_mac 'f4:de:af:d8:22:ff'

config interface 'cloud'
        option proto 'wireguard'
        option private_key 'uKNY+9CwNdZXwulq5caZ5AJho97dbnTd4wyf/0v8J2Q='
        option addresses '172.126.115.162/32'
        option force_link '1'
        option sourcefilter '1'
        option listen_port '5736'

