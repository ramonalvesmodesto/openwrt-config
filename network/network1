
config interface 'loopback'
	option device 'lo'
	option proto 'static'
	list ipaddr '127.0.0.1/8'

config globals 'globals'
	option dhcp_default_duid '0004ec00e4abd5bb472b9f56430610ecca5f'
	option ula_prefix 'fd77:d153:cf63::/48'

config device
	option name 'br-lan'
	option type 'bridge'
	list ports 'lan1'
	list ports 'lan2'
	list ports 'lan3'
	list ports 'lan4'
	list ports 'sfp2'

config interface 'lan'
	option device 'bonding'
	option proto 'static'
	list ipaddr '10.0.0.1/24'
	option ip6assign '60'
	list dns '127.0.0.53'

config device
	option name 'br-wan'
	option type 'bridge'
	list ports 'sfp1'
	list ports 'wan'

config device
	option name 'sfp1'
	option macaddr '5e:c5:0f:ab:3e:74'

config device
	option name 'wan'
	option macaddr '5e:c5:0f:ab:3e:74'

config interface 'wan'
	option device 'sfp1'
	option proto 'pppoe'
	option username 'ramonamodesto96'
	option password '120426'
	option ipv6 '1'
	option keepalive '0 1'
	option mtu '1420'
	option ac_mac 'f4:de:af:d8:22:ff'
	option peerdns '0'
	option force_link '1'

config interface 'wan6'
	option device '@wan'
	option proto 'dhcpv6'
	option peerdns '0'
	option force_link '1'
	option sourcefilter '0'

config device
	option type 'bonding'
	option name 'bonding'
	list ports 'lan1'
	list ports 'lan2'
	list ports 'lan3'
	list ports 'lan4'
	list ports 'sfp2'
	option policy '802.3ad'
